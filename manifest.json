{
  "manifest_version": 2,
  "name": "aws-sniffer",
  "description": "extracts the session data in AWS console",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://console.aws.amazon.com/*"
      ],
      "js": [
        "sniffer.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "cookies",
    "*://console.aws.amazon.com/*"
  ]
}